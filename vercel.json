{
    "headers": [
        {
            "source": "/sitemap.xml",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "noindex"
                },
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}